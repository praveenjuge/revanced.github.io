{
  "devDependencies": {
    "@playwright/test": "^1.22.2",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@smui/button": "^6.0.0-beta.16",
    "@smui/circular-progress": "^6.0.0-beta.16",
    "@smui/drawer": "^6.0.0-beta.16",
    "@smui/list": "^6.0.0-beta.16",
    "@smui/tab": "^6.0.0-beta.15",
    "@smui/tab-bar": "^6.0.0-beta.16",
    "@steeze-ui/simple-icons": "^1.0.0-next.0",
    "@steeze-ui/svelte-icon": "^1.3.1",
    "@sveltejs/adapter-auto": "^1.0.0-next.44",
    "@sveltejs/kit": "^1.0.0-next.330",
    "@types/dompurify": "^2.3.3",
    "@types/marked": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "marked": "^4.0.16",
    "mdsvex": "^0.10.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.13",
    "postcss-load-config": "^4.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "smui-theme": "^6.0.0-beta.16",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "tailwindcss": "^3.0.24",
    "tslib": "^2.4.0",
    "typescript": "~4.6.4"
  },
  "name": "revanced-website",
  "private": true,
  "scripts": {
    "build": "svelte-kit build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "svelte-kit dev",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "package": "svelte-kit package",
    "prepare": "npm run theme && svelte-kit sync",
    "preview": "svelte-kit preview",
    "test": "playwright test",
    "theme": "run-p -l theme:*",
    "theme:dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
    "theme:light": "smui-theme compile static/smui.css -i src/theme"
  },
  "type": "module",
  "version": "0.0.1"
}
